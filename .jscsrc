{
  "preset": "google",
  "maxErrors": -1,
  "disallowSpacesInAnonymousFunctionExpression": null,
  "maximumLineLength": null,
  "disallowSpaceAfterKeywords": [
    "if",
    "for",
    "while",
    "switch",
    "catch"
  ],
  "requireSpaceAfterKeywords": [
    "else",
    "do",
    "return",
    "try"
  ],
  "excludeFiles": ["node_modules/**"]
}
